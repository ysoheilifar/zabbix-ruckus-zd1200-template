            <items>
                <item>
                    <uuid>eaad1df88ad04a8b8a51a43b65eb4755</uuid>
                    <name>ICMP ping</name>
                    <type>SIMPLE</type>
                    <key>icmpping</key>
                    <delay>60</delay>
                    <history>7d</history>
                    <valuemap>
                        <name>Service state</name>
                    </valuemap>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>ICMP</value>
                        </tag>
                    </tags>
                    <triggers>
                        <trigger>
                            <uuid>18c18e4e83be4e089fe73064ab835782</uuid>
                            <expression>max(/SNMP Ruckus Zonedirector/icmpping,#3)=0</expression>
                            <name>{HOST.NAME} is unavailable by ICMP</name>
                            <priority>AVERAGE</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <uuid>8cb566e355ba4c37b53d8effa8349403</uuid>
                    <name>ICMP loss</name>
                    <type>SIMPLE</type>
                    <key>icmppingloss</key>
                    <delay>60</delay>
                    <history>7d</history>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>ICMP</value>
                        </tag>
                    </tags>
                    <triggers>
                        <trigger>
                            <uuid>bc098cecd39d4720a7049d5177208371</uuid>
                            <expression>min(/SNMP Ruckus Zonedirector/icmppingloss,5m)&gt;20</expression>
                            <name>Ping loss is too high on {HOST.NAME}</name>
                            <priority>WARNING</priority>
                            <dependencies>
                                <dependency>
                                    <name>{HOST.NAME} is unavailable by ICMP</name>
                                    <expression>max(/SNMP Ruckus Zonedirector/icmpping,#3)=0</expression>
                                </dependency>
                            </dependencies>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <uuid>0b1ec4fb079c4c9bb71316c3be383d3f</uuid>
                    <name>ICMP response time</name>
                    <type>SIMPLE</type>
                    <key>icmppingsec</key>
                    <delay>60</delay>
                    <history>7d</history>
                    <value_type>FLOAT</value_type>
                    <units>s</units>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>ICMP</value>
                        </tag>
                    </tags>
                    <triggers>
                        <trigger>
                            <uuid>3fb6d55c91224a7ea07ac86d3245b237</uuid>
                            <expression>avg(/SNMP Ruckus Zonedirector/icmppingsec,5m)&gt;0.5</expression>
                            <name>Response time is too high on {HOST.NAME}</name>
                            <priority>WARNING</priority>
                            <dependencies>
                                <dependency>
                                    <name>{HOST.NAME} is unavailable by ICMP</name>
                                    <expression>max(/SNMP Ruckus Zonedirector/icmpping,#3)=0</expression>
                                </dependency>
                            </dependencies>
                        </trigger>
                    </triggers>
                </item>
